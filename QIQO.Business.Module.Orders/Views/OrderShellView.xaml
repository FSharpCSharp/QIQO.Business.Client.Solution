<UserControl x:Class="QIQO.Business.Module.Orders.Views.OrderShellView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:prism="http://prismlibrary.com/"
             xmlns:inf="clr-namespace:QIQO.Business.Client.Core.Infrastructure;assembly=QIQO.Business.Client.Core"
             xmlns:local="clr-namespace:QIQO.Business.Module.Orders.Views">
    <UserControl.Resources>
        <ResourceDictionary Source="pack://application:,,,/QIQO.Business.Client.Core.UI;component/Resources/WindowResources.xaml"/>
    </UserControl.Resources>
    <DockPanel LastChildFill="True">
        <!--<StackPanel Orientation="Horizontal" DockPanel.Dock="Top" prism:RegionManager.RegionName="{x:Static inf:RegionNames.OrdersNavigationRegion}"/>-->

        <TabControl prism:RegionManager.RegionName="{x:Static inf:RegionNames.OrdersRegion}"  ItemContainerStyle="{StaticResource ClosableTabItem}" Background="Transparent">
            
        </TabControl>


    </DockPanel>
</UserControl>
